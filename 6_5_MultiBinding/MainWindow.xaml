<Window x:Class="_6_5_MultiBinding.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_6_5_MultiBinding"
        mc:Ignorable="d"
        Title="MainWindow" Height="200" Width="300">
    <StackPanel Background="LightBlue" Height="184" VerticalAlignment="Center">
        <TextBox x:Name="nameBox1" Height="23" Margin="5 10 5 5"/>
        <TextBox x:Name="nameBox2" Height="23" Margin="5"/>
        <TextBox x:Name="emailBox1" Height="23" Margin="5"/>
        <TextBox x:Name="emailBox2" Height="23" Margin="5"/>
        <Button x:Name="button1" Content="Submit" Width="80" Margin="5"/>
    </StackPanel>
</Window>
